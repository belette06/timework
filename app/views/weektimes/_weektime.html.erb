 <%if @weektimes.empty? %> 
  <tr>
    <td colspan="4", class="text-center"><strong>Aucune de feuille d'heure ajout√©</strong></td>
  </tr>
 <% else %>   
   <% @weektimes.each do |weektime| %>
   
     <tr scope="row">
      <td class="text-center"><%= weektime.numsemaine %> | <%= weektime.dateweek.strftime("%Y-%m") %> </td>
      <td class="text-center"><%= weektime.workweek %> Hrs </td>
      <td class="text-center"><%= weektime.accord %></td>
      <td class="text-center"> </td> 
      <td class="text-center">  <%= link_to "Voir", weektime, class:"btn btn-outline-info btn-sm"%></td>
     </tr>

   <% end %>    
 <%end%>  
